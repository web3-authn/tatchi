{
  "name": "wallet-secure",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "predev": "node ./scripts/generate-home-snippets.mjs",
    "dev": "concurrently --kill-others --names docs,caddy \"vitepress dev src --port 5222\" \"sh run_caddy.sh\"",
    "dev:iphone": "concurrently --kill-others \"vitepress dev src --port 5222\" \"sh run_caddy.sh\"",
    "gen:home-snippets": "node ./scripts/generate-home-snippets.mjs",
    "prebuild": "node ./scripts/generate-home-snippets.mjs",
    "build": "vitepress build src --outDir dist/docs",
    "preview": "vitepress preview dist/docs --port 5222"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.76.1",
    "@tatchi-xyz/sdk": "workspace:*",
    "borsh": "^2.0.0",
    "bs58": "^6.0.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.542.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sonner": "^1.5.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^4.4.1",
    "concurrently": "^9.1.2",
    "globals": "^16.0.0",
    "shiki": "^2.5.0",
    "typescript": "~5.8.3",
    "unenv": "^1.10.0",
    "vite": "^6.3.5",
    "vite-plugin-mkcert": "^1.17.8",
    "vite-plugin-node-polyfills": "^0.24.0"
  }
}
