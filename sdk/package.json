{
  "name": "@tatchi-xyz/sdk",
  "version": "0.1.0",
  "type": "module",
  "description": "An embedded passkey wallet SDK built on NEAR protocol",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/src/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/src/index.d.ts"
    },
    "./plugins/headers": {
      "import": "./dist/esm/plugins/headers.js",
      "require": "./dist/cjs/plugins/headers.js",
      "types": "./dist/types/src/plugins/headers.d.ts"
    },
    "./plugins/next": {
      "import": "./dist/esm/plugins/next.js",
      "require": "./dist/cjs/plugins/next.js",
      "types": "./dist/types/src/plugins/next.d.ts"
    },
    "./core": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/src/index.d.ts"
    },
    "./plugins/vite": {
      "import": "./dist/esm/plugins/vite.js",
      "require": "./dist/cjs/plugins/vite.js",
      "types": "./dist/types/src/plugins/vite.d.ts"
    },
    "./server": {
      "import": "./dist/esm/server/index.js",
      "require": "./dist/cjs/server/index.js",
      "types": "./dist/types/src/server/index.d.ts"
    },
    "./server/router/express": {
      "import": "./dist/esm/server/router/express.js",
      "require": "./dist/cjs/server/router/express.js",
      "types": "./dist/types/src/server/router/express-adaptor.d.ts"
    },
    "./server/router/cloudflare": {
      "import": "./dist/esm/server/router/cloudflare.js",
      "require": "./dist/cjs/server/router/cloudflare.js",
      "types": "./dist/types/src/server/router/cloudflare-adaptor.d.ts"
    },
    "./server/wasm/signer": {
      "import": "./dist/esm/server/wasm/signer.js",
      "require": "./dist/cjs/server/wasm/signer.js",
      "types": "./dist/types/src/server/wasm/signer.d.ts"
    },
    "./server/wasm/vrf": {
      "import": "./dist/esm/server/wasm/vrf.js",
      "require": "./dist/cjs/server/wasm/vrf.js",
      "types": "./dist/types/src/server/wasm/vrf.d.ts"
    },
    "./react": {
      "import": "./dist/esm/react/index.js",
      "require": "./dist/cjs/react/index.js",
      "types": "./dist/types/src/react/index.d.ts"
    },
    "./react/styles": "./dist/esm/react/styles/styles.css",
    "./WalletIframe/client/html": {
      "import": "./dist/esm/core/WalletIframe/client/html.js",
      "require": "./dist/cjs/core/WalletIframe/client/html.js",
      "types": "./dist/types/src/core/WalletIframe/client/html.d.ts"
    },
    "./worker": "./dist/web3authn-signer.worker.js",
    "./wasm": "./dist/wasm_signer_worker_bg.wasm",
    "./wasm-js": "./dist/wasm_signer_worker.js",
    "./components/modal": {
      "import": "./dist/esm/core/WebAuthnManager/Components/modal.js",
      "require": "./dist/cjs/core/WebAuthnManager/Components/modal.js",
      "types": "./dist/types/src/core/WebAuthnManager/Components/modal.d.ts"
    },
    "./components/embedded": {
      "import": "./dist/esm/core/WebAuthnManager/Components/embedded.js",
      "require": "./dist/cjs/core/WebAuthnManager/Components/embedded.js",
      "types": "./dist/types/src/core/WebAuthnManager/Components/embedded.d.ts"
    }
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "typesVersions": {
    "*": {
      "react": [
        "dist/types/src/react/index.d.ts"
      ],
      "react/*": [
        "dist/types/src/react/*"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "./scripts/build-dev.sh",
    "build:prod": "./scripts/build-prod.sh",
    "build:rolldown": "rolldown -c rolldown.config.ts",
    "dev": "rolldown -c rolldown.config.ts -w",
    "test": "npm run build:check:fresh || npm run build && USE_RELAY_SERVER=1 playwright test --reporter=line",
    "test:inline": "npm run build:check:fresh || npm run build && USE_RELAY_SERVER=1 playwright test --reporter=line",
    "test:e2e": "playwright test src/__tests__/e2e --reporter=line",
    "test:unit": "playwright test src/__tests__/unit --reporter=line",
    "test:wallet-iframe": "USE_RELAY_SERVER=0 playwright test src/__tests__/wallet-iframe --reporter=line",
    "test:lit-components": "USE_RELAY_SERVER=0 playwright test src/__tests__/lit-components --reporter=line",
    "build:check:fresh": "./scripts/check-build-freshness.sh",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "NEAR",
    "passkey",
    "webauthn",
    "authentication",
    "blockchain",
    "react",
    "typescript",
    "sdk"
  ],
  "author": "web3authn.org",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/web3-authn/sdk.git"
  },
  "bugs": {
    "url": "https://github.com/web3-authn/sdk/issues"
  },
  "homepage": "https://github.com/web3-authn",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "lucide-react": ">=0.541.0",
    "qrcode": ">=1.5.4",
    "jsqr": ">=1.4.0",
    "express": ">=4.17.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "lucide-react": {
      "optional": true
    },
    "qrcode": {
      "optional": true
    },
    "jsqr": {
      "optional": true
    },
    "express": {
      "optional": true
    }
  },
  "dependencies": {
    "@lit/react": "^1.0.8",
    "@noble/ed25519": "^2.1.0",
    "idb": "^8.0.3",
    "bs58": "^6.0.0",
    "lit": "^3.3.1",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@near-js/types": "^2.0.1",
    "@types/qrcode": "^1.5.5",
    "@types/express": "^4.17.21",
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.19.9",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "borsh": "^2.0.0",
    "js-sha256": "^0.11.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rolldown": "^1.0.0-beta.27",
    "rollup-plugin-css-only": "^4.5.2",
    "typescript": "^5.8.3",
    "express": "^4.21.2"
  }
}
